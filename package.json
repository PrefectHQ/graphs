{
  "name": "@prefecthq/graphs",
  "private": false,
  "version": "2.0.5",
  "description": "Large scale graphs designed for Prefect",
  "scripts": {
    "serve": "vite --host --mode=demo",
    "dev": "vue-tsc -w & tsc-alias -w & vite build -w",
    "build": "vue-tsc && tsc-alias && vite build",
    "build:demo": "vite build --mode=demo",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "changelog": "auto-changelog --package --commit-limit 0",
    "version": "npm run changelog && git add CHANGELOG.md"
  },
  "main": "./dist/graphs.umd.js",
  "module": "./dist/graphs.mjs",
  "exports": {
    ".": {
      "import": "./dist/graphs.mjs",
      "require": "./dist/graphs.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "types": "./dist/types/src/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PrefectHQ/graphs.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PrefectHQ/graphs/issues"
  },
  "homepage": "https://github.com/PrefectHQ/graphs#readme",
  "devDependencies": {
    "@prefecthq/eslint-config": "1.0.31",
    "@types/d3": "7.4.1",
    "@types/node": "^20.1.7",
    "@vitejs/plugin-vue": "4.3.4",
    "auto-changelog": "^2.4.0",
    "autoprefixer": "10.4.16",
    "eslint": "8.50.0",
    "postcss": "8.4.19",
    "tailwindcss": "3.3.3",
    "tsc-alias": "1.8.8",
    "typescript": "5.2.2",
    "vite": "4.4.9",
    "vite-svg-loader": "^4.0.0",
    "vue-tsc": "1.8.15"
  },
  "peerDependencies": {
    "@prefecthq/prefect-design": "^2.0.1",
    "@prefecthq/vue-compositions": "^1.5.9",
    "vue": "^3.2.45",
    "vue-router": "^4.0.12"
  },
  "dependencies": {
    "d3": "7.8.5",
    "date-fns": "2.30.0",
    "mitt": "3.0.1",
    "pixi-viewport": "5.0.2",
    "pixi.js": "7.3.1"
  }
}
